operation transformComplexType(e : Any, sb : Any, context : Map) {
	if (e.a_name.length > 0) {
		sb.append("public class " + e.a_name);sb.append(" {");newline(sb);
	}
	transformChildrenWsdl(sb, context, e);
	if (e.a_name.length > 0) {
		sb.append("}");newline(sb);
	}
}

