operation transformMessages()  {
//sb.append("================ transformMessages1 B =================\n");
	for (mod in allModels) {
		for (e in mod!t_message.allInstances()) {
 			transformWsdl(e, sb, context);
 		}
	}
//sb.append("================ transformMessages1 e =================\n");
}

