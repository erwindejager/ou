operation transformSequence(e : Any, sb : Any, context : Map) {
	if (e.a_maxOccurs <> null and e.a_maxOccurs.length() > 0) {
		addCollection(context, "WSDL");
	}
	transformChildrenWsdl(sb, context, e);
}

